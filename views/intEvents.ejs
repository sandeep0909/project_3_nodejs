
  <div id="events">
    <div class="container">
    <div id="event-btn">
      <a href='/users/<%= user.id%>/events/new' class="btn btn-lg btn-primary" >Add Event</a><hr>
    </div>


      <% events.forEach(function(event){ %>
        <div class="col-sm-4 col-lg-4 col-md-4">
            <div class="thumbnail tile">
                <img class="" src="<%= event.img || '/images/defaultImage.jpg'%>" alt="">
                <div class="caption">
                    <h4 class="pull-right"><%= event.eDate %></h4>
                    <h4><a href='/events/<%= event.id%>'><%= event.name %></a>
                    </h4>
                    <p id="event-desc"><%= event.description %></p>
                </div>
            </div>
        </div>

      <% }) %>

    </div>
    <!-- /.container -->

        <hr>

        <!-- Footer -->
        <footer>
            <div class="row">
                <div class="col-lg-12">
                    <p>Copyright &copy; Event Hub 2016</p>
                </div>
            </div>
        </footer>

    </div>

<script type="text/javascript">
  $('body').css('background-image', 'url("/images/rainbow.png")')
</script>

<%# </body>

</html>%>
